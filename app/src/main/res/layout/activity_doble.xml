<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layoutt"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:openDrawer="start"
    tools:context=".doble"
    android:background="@drawable/registro">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_marginTop="12dp"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:id="@+id/salir"
            android:background="@drawable/atras2"
            />
        <ImageView

            android:layout_width="150dp"
            android:layout_height="150dp"
            android:id="@+id/prin"
            android:layout_centerHorizontal="true"
            android:background="@drawable/aquascale"
            android:paddingEnd="50sp"/>

        <View
            android:id="@+id/busto"
            android:layout_width="match_parent"
            android:layout_height="90sp"
            android:layout_below="@+id/prin" />
        <TextView
            android:layout_width="520dp"
            android:layout_height="wrap_content"
            android:id="@+id/vista"
            android:text="@string/reg_strate"
            android:textColor="#FFFFFF"
            android:textAlignment="center"
            android:textStyle="bold"
            android:textSize="30sp"
            android:fontFamily="@font/texto"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/busto"
            />

        <TextView

            android:padding="1dp"
            android:layout_width="520dp"
            android:layout_height="wrap_content"
            android:id="@+id/vis"
            android:text="AQUASCALE "
            android:textColor="#FFFFFF"
            android:textAlignment="center"
            android:textStyle="bold"
            android:textSize="15sp"
            android:fontFamily="@font/texto"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/vista"
            />
        <View
            android:layout_width="match_parent"
            android:layout_height="25sp"
            android:id="@+id/bust"
            android:layout_below="@+id/vis"/>
        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="match_parent"
            android:layout_below="@+id/bust"
            android:layout_height="wrap_content"
            android:id="@+id/txt_name4"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:textColorHint="@color/black"
            app:endIconTint="@color/black"
            app:endIconMode="custom"
            app:endIconDrawable="@drawable/ic_email"
            android:background="@drawable/rounded_borders"
            android:layout_marginEnd="20dp"
            android:layout_marginStart="20dp"
            android:backgroundTint="@color/colorb">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/txt_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/bust"
                android:fontFamily="sans-serif-condensed-medium"
                android:textColor="#000000"
                android:backgroundTint="@color/white"
                android:singleLine="true"
                android:hint="CORREO ELECTRÓNICO"
                android:inputType="textEmailAddress"
                android:background="@android:color/transparent"/>

        </com.google.android.material.textfield.TextInputLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="25sp"
            android:id="@+id/bustt"
            android:layout_below="@+id/txt_name4"/>
        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/bustt"
            android:id="@+id/clas2"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:textColorHint="@color/black"
            app:endIconTint="@color/black"
            app:endIconMode="custom"
            app:endIconDrawable="@drawable/ic_user"
            android:background="@drawable/rounded_borders"
            android:layout_marginEnd="20dp"
            android:layout_marginStart="20dp"
            android:backgroundTint="@color/colorb">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/txt_namee"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/bustt"
                android:fontFamily="sans-serif-condensed-medium"

                android:textColor="#000000"
                android:hint="USUARIO"
                android:inputType="text"
                android:backgroundTint="@color/white"
                android:singleLine="true"
                android:background="@android:color/transparent"/>
        </com.google.android.material.textfield.TextInputLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="25sp"
            android:id="@+id/busttt"
            android:layout_below="@+id/clas2"/>
        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/clas3"
            android:layout_below="@id/busttt"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:textColorHint="@color/black"
            app:endIconMode="password_toggle"
            app:endIconTint="@color/black"
            android:background="@drawable/rounded_borders"
            android:layout_marginEnd="20dp"
            android:layout_marginStart="20dp"
            android:backgroundTint="@color/colorb">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/txt_name2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/busttt"
                android:fontFamily="sans-serif-condensed-medium"

                android:textColor="#000000"
                android:hint="CONTRASEÑA"
                android:inputType="textPassword"
                android:backgroundTint="@color/white"
                android:singleLine="true"
                android:background="@android:color/transparent"/>
        </com.google.android.material.textfield.TextInputLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="25sp"
            android:id="@+id/bustttt"
            android:layout_below="@+id/clas3"/>
        <com.google.android.material.textfield.TextInputLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/clas4"
            android:layout_below="@id/bustttt"
            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox"
            android:textColorHint="@color/black"
            app:endIconMode="password_toggle"
            app:endIconTint="@color/black"
            android:background="@drawable/rounded_borders"
            android:layout_marginEnd="20dp"
            android:layout_marginStart="20dp"
            android:backgroundTint="@color/colorb">
            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/txt_name3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/bustttt"
                android:fontFamily="sans-serif-condensed-medium"

                android:textColor="#000000"
                android:hint="CONFIRMAR CONTRASEÑA"
                android:inputType="textPassword"
                android:backgroundTint="@color/white"
                android:singleLine="true"
                android:background="@android:color/transparent"/>
        </com.google.android.material.textfield.TextInputLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="15sp"
            android:id="@+id/busttttr"
            android:layout_below="@+id/clas4"/>
        <Button
            android:id="@+id/btn_send_dataaa"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/busttttr"
            android:layout_marginTop="10dp"
            android:backgroundTint="@color/white"
            android:textColor="#094070"
            android:elevation="15dp"
            android:text="REGISTRADO"
            android:textSize="14sp"
            app:cornerRadius="20sp"
            android:layout_marginEnd="90dp"
            android:layout_marginStart="90dp"/>

    </RelativeLayout>
</androidx.drawerlayout.widget.DrawerLayout>